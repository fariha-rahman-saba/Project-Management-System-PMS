{
  "version": 2,
  "builds": [
    {
      "src": "pms/wsgi.py",
      "use": "@vercel/python",
      "config": {"maxLambdaSize":"15mb", "runtime":"python3.9"}
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "pms/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ]
  
}
